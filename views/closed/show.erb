<h1>Closed Table: <%= @party.name %></h1>
<table>
	<tr>
		<th>Menu Item</th>
		<th>Price</th>
	</tr>
	<% @orders.each do |order|%>
	<tr>
		<td><%= order.item_name %></td>
		<td><%= order.item_price %></td>
	</tr>
	<% end %>
	<tr>
		<td>Total</td>
		<td><%= @party.total_price %></td>
	</tr>
</table>

